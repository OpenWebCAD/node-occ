{
  "name": "node-occ",
  "version": "1.0.1",
  "description": "OpenCascade OCCT Wrapper for Node js",
  "preferGlobal": true,
  "dependencies": {
    "@types/should": "^13.0.0",
    "async": "^3.2.4",
    "colors": "*",
    "gettemporaryfilepath": "1.0.1",
    "nan": "^2.16.0",
    "@mapbox/node-pre-gyp": "^1.0.10",
    "pace": "~0.0.4",
    "progress": "~2.0.3",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/async": "^3.2.20",
    "@types/mocha": "^9.1.1",
    "@types/node": "^20.2.3",
    "eslint": "^8.23.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-require-path-exists": "^1.1.9",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-utils": "^3.0.0",
    "mocha": "^10.0.0",
    "should": "~13.2.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": {
    "name": "Etienne Rossignon"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "3D",
    "CAD",
    "OpenCascade",
    "OCE",
    "Solid Modeler",
    "geometry",
    "BRep"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/openwebcad/node-occ.git"
  },
  "scripts": {
    "build": "tsc ",
    "install": "./node_modules/.bin/node-pre-gyp install --fallback-to-build",
    "test": "mocha test/*.ts",
    "lint": "eslint lib test bin",
    "doc": "grunt doc",
    "postinstall": "node post-install.js"
  },
  "binary": {
    "module_name": "occ",
    "module_path": "./lib/binding/",
    "host-aws": "https://nodeocc.s3-us-west-1.amazonaws.com",
    "host": "https://github.com/openwebcad/node-occ/releases/download/",
    "remote_path": "{version}"
  },
  "gypfile": true,
  "bugs": {
    "url": "https://github.com/openwebcad/node-occ/issues"
  },
  "directories": {
    "test": "test"
  },
  "license": "MIT"
}